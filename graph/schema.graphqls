# GraphQL schema example
#
# https://gqlgen.com/getting-started/
scalar Map

type Node {
  id: ID!
  type: String!
  attributes: Map!
}

input QueryNodes {
  type: String!
}

type Query {
  nodes(input: QueryNodes!): [Node!]!
}

input NewNode {
  type: String!
  attributes: Map!
}

type Mutation {
  createNode(input: NewNode!): Node!
}